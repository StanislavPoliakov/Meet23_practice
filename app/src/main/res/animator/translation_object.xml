        android:startOffset="800"
        android:duration="15"
        android:propertyName="X"
        android:valueFrom="0.0"
        android:valueTo="-400.0"
        android:valueType="floatType"/>
    <objectAnimator
        android:duration="15"
        android:propertyName="X"
        android:valueFrom="-400.0"
        android:valueTo="0.0"
        android:valueType="floatType"/>
    <objectAnimator
        android:startOffset="100"
        android:duration="15"
        android:propertyName="Y"
        android:valueFrom="0.0"
        android:valueTo="-400.0"
        android:valueType="floatType"/>
